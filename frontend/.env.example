# Webhook Configuration
WEBHOOK_SECRET=your-secure-webhook-secret-key-here
WEBHOOK_TIMEOUT=30000
WEBHOOK_MAX_PAYLOAD_SIZE=10485760

# Rate Limiting Configuration
RATE_LIMIT_WINDOW=60000
RATE_LIMIT_MAX_REQUESTS=100

# Duplicate Detection Configuration
DUPLICATE_DETECTION_WINDOW=600000

# Server Configuration
PORT=8080
NODE_ENV=development

# Logging Configuration
LOG_LEVEL=info
LOG_WEBHOOK_REQUESTS=true
LOG_WEBHOOK_RESPONSES=true

# Security Configuration
ENABLE_WEBHOOK_SIGNATURE_VERIFICATION=true
ENABLE_RATE_LIMITING=true
ENABLE_DUPLICATE_DETECTION=true

# Monitoring Configuration
ENABLE_WEBHOOK_MONITORING=true
WEBHOOK_HEALTH_CHECK_INTERVAL=60000

# Lleverage.ai Configuration
LLEVERAGE_AI_WEBHOOK_URL=https://api.lleverage.ai/webhook/callback
LLEVERAGE_AI_API_KEY=your-lleverage-ai-api-key

# Database Configuration (if using persistent storage)
# DATABASE_URL=postgresql://user:password@localhost:5432/webhook_db
# REDIS_URL=redis://localhost:6379

# External Services
WEBHOOK_SITE_URL=https://webhook.site/9c9dd13f-b6f0-41f4-a647-cb10bd929677

# Testing Configuration
TEST_WEBHOOK_URL=http://localhost:8080/webhook/visualization-data
TEST_ITERATIONS=5